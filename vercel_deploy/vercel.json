{
  "version": 2,
  "name": "casamx-streamlit-app",
  "builds": [
    {
      "src": "streamlit_simple.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit_simple.py"
    }
  ],
  "env": {
    "STREAMLIT_SERVER_PORT": "8080",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0",
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": "false",
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_ENABLE_CORS": "false"
  },
  "functions": {
    "streamlit_simple.py": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": false,
    "autoAlias": false
  }
}